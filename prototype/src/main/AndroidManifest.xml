<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prototype"
    xmlns:tools="http://schemas.android.com/tools" >
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-sdk tools:overrideLibrary="android.support.v17.leanback"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_removebg_preview"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Prototype">
        <activity android:name=".Camera"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Prototype.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TypeAccount"
            android:label="@string/type" />
        <activity
            android:name=".LoginPilgrim"
            android:label="@string/login" />
        <activity
            android:name=".information"
            android:label="@string/app_name" />
        <activity
            android:name=".pilgrimPage"
            android:label="@string/Hello" />
    </application>



</manifest>